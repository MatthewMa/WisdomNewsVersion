            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.GuideActivity">
        </activity>
        <activity android:name=".activities.HomeActivity">
        </activity>
    </application>

</manifest>
